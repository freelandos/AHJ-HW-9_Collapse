{
  "extends": "airbnb-base",
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "jest": true
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-restricted-syntax": [
      "error",
      // "ForInStatement",
      // "ForOfStatement",
      "LabeledStatement",
      "WithStatement"
    ]
  }
}
